          <mj-column width="50%">
            <mj-text mj-class="p small">
              Napisz:<br>
              <a href="mailto:pomoc.techniczna@wolterskluwer.pl" style="color:#007ac3;" class="blue-link" target="_blank">pomoc.techniczna@wolterskluwer.pl</a>
            </mj-text>
          </mj-column>
        </mj-section>

        <!-- Eloqua Footer -->
        <mj-include path="../WK Include/elq footer.mjml" />

      </mj-wrapper>
