<mjml>
  <mj-head>

    <!-- Brandbook font for compliant ESP's -->
    <mj-font name="Fira Sans" href="https://fonts.googleapis.com/css?family=Fira+Sans:400,400i,700" />

    <!-- MJML Styles -->
    <mj-attributes>
      <mj-all padding="0px" line-height="1.2" />
      <mj-column width="90%" vertical-align="top" />
      <mj-text font-family="Fira Sans,Arial,sans-serif" />
      <mj-button font-family="Fira Sans,Arial,sans-serif" />
      <mj-class name="h" color="#007ac3" font-size="20px" padding="10px 25px 10px 25px" />
      <mj-class name="p" color="#767676" font-size="16px" padding="10px 25px 10px 25px" />
      <mj-class name="u" color="#767676" font-size="16px" padding-right="25px" padding-left="5px" />
      <mj-class name="over" background-color="#007ac3" color="#ffffff" font-size="20px" align="left" vertical-align="middle" border="none" border-radius="0px" inner-padding="15px" />
      <mj-class name="bar" background-color="#e5202e" color="#ffffff" font-size="2" align="right" vertical-align="middle" border="none" border-radius="0px" inner-padding="2px" />

      <mj-class name="big" font-size="32px" />
      <mj-class name="small" font-size="14px" />
      <mj-class name="center" align="center" />
      <mj-class name="left" align="left" />
      <mj-class name="right" align="right" />
      <mj-class name="bold" font-weight="bold" />
      <mj-class name="white" color="#ffffff" />
      <mj-class name="blue" color="#007ac3" />
      <mj-class name="grey" color="#767676" />
      <mj-class name="bg-blue" background-color="#2a7ac3" />
      <mj-class name="bg-red" background-color="#e5202e" />
      <mj-class name="bg-green" background-color="#85bc20" />
      <mj-class name="bg-white" background-color="#ffffff" />
      <mj-class name="bg-grey" background-color="#f0f0f0" />

      <mj-class name="cta" background-color="#85bc20" color="#ffffff" font-size="24px" align="right" vertical-align="middle" border="none" border-radius="0px" inner-padding="15px 30px 15px 30px" href="https://szkolenia.wolterskluwer.pl/" />
      <mj-image href="https://szkolenia.wolterskluwer.pl/" />
    </mj-attributes>

    <!-- HTML Styles -->
    <mj-style inline="inline">
      span.preheader { display:none !important; font-size:1px; color:#ffffff; line-height:1px; max-height:0px; max-width:0px; opacity:0; overflow:hidden; }
    </mj-style>
    <mj-style>
      .table { display: block !important; width: 90% !important; padding: 2px 10px 2px 10px !important; background-color:#ffffff !important; color:#767676 !important; font-size: 18px !important; } .icon { width: 20% !important; padding: 2px 10px 2px 10px !important;
      text-align: center !important; background-color:#ffffff !important; vertical-align: middle !important; } .white-link a:link, span.MsoHyperlink { mso-style-priority:100 !important; color:#ffffff !important; } .blue-link a:link, span.MsoHyperlink
      { mso-style-priority:100 !important; color:#007ac3 !important; } a { mso-style-priority:100 !important; text-decoration: none !important; }
    </mj-style>

  </mj-head>
  <mj-body>
    <mj-container mj-class="bg-grey">

      <!-- Fix for Outlook changing all fonts to Arial -->
      <mj-include path="../WK Include/outlook font issue.mjml" />

      <!-- Pre-header -->
      <mj-section>
        <mj-column>
          <mj-raw>
            <!--[if !mso]><!-->
            <p>
              <!--<![endif]-->

              <span class="preheader">Ekspercka wiedza na wyciągnięcie ręki
                <mj-include path="../WK Include/post-header.mjml" />
              </span>

              <!--[if !mso]><!-->
            </p>
            <!--<![endif]-->
          </mj-raw>
        </mj-column>
      </mj-section>

      <!-- Logo and View in browser -->
      <mj-section padding-bottom="20px">
