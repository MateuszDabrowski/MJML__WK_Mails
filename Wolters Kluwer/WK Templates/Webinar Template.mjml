            <mj-button mj-class="bar bg-blue left">
              <a href="https://szkolenia.wolterskluwer.pl/konto-uzytkownika/ustawienia-konta/zarejestruj.html" class="white-link" style="color: #fffffe;">Logowanie</a> | <a href="https://szkolenia.wolterskluwer.pl/kontakt/" class="white-link" style="color: #fffffe;">Kontakt</a>
            </mj-button>
          </mj-column>
          <mj-column mj-class="bg-blue" width="50%">
